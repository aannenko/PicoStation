{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SerialPortService": {
    "PortName": "/dev/ttyACM0",
    "ReadTimeout": "00:00:00.05",
    "WriteTimeout": "00:00:00.1",
    "DtrEnable": true,
    "Encoding": "UTF8",
    "NewLine": "\r\n"
  },
  "PrometheusService": {
    "SerialPortCommand": "print_readings('prometheus')",
    "SerialPortEndOfResponse": "\r\n>>>"
  }
}
